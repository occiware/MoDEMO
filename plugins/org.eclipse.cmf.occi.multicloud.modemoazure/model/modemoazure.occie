<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="modemoazure" scheme="http://org.eclipse.cmf.occi.multicloud.modemoazure#">
  <import href="http://schemas.ogf.org/occi/core#/"/>
  <import href="http://schemas.ogf.org/occi/infrastructure#/"/>
  <kinds name="AzureVM" scheme="http://org.eclipse.cmf.occi.multicloud.modemoazure#">
    <attributes name="azure.vmazure.id" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="azure.vmazure.imageID" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="azure.vmazure.publicIP" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="azure.vmazure.sizeType" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="azure.vmazure.resourceGroup" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <actions name="powerOff" scheme="http://org.eclipse.cmf.occi.multicloud.modemoazure/azurevm/action#"/>
    <actions name="restart" scheme="http://org.eclipse.cmf.occi.multicloud.modemoazure/azurevm/action#"/>
    <parent href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='compute']"/>
  </kinds>
</occi:Extension>
