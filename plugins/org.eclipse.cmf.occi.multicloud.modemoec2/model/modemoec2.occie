<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="modemoec2" scheme="http://org.eclipse.cmf.occi.multicloud.modemoec2#">
  <import href="http://schemas.ogf.org/occi/core#/"/>
