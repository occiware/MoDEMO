  <kinds name="ec2" scheme="http://org.eclipse.cmf.occi.multicloud.modemoec2#">
    <attributes name="aws.ec2instance.key" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="aws.ec2instance.publicIP" mutable="true">
